{
  "schemaVersion": 1,
  "id": "fabricdiscord",
  "version": "${version}",

  "name": "Fabric Discord",
  "description": "A basic Fabric mod that links a server to a discord channel",
  "authors": [
    "Cloud"
  ],

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "server": [
      "fr.cloud.fabricdiscord.FabricDiscord"
    ]
  },
  "mixins": [
    "fabricdiscord.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
